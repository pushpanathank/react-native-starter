{
  "name": "Mahizh",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "debug": "cd android && gradlew assembleDebug && cd ..",
    "assemble": "npm run bundle && npm run debug",
    "clean": "cd android && gradlew clean && cd ..",
    "cleanrun": "npm run clean && npm run android",
    "codepush-add": "code-push app add Mahizh android react-native",
    "codepush-p": "code-push release-react Mahizh android -d Production",
    "codepush-s": "code-push release-react Mahizh android -d Staging"
  },
